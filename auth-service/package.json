      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "node": true,
      "es2021": true
    },
    "rules": {
      "@typescript-eslint/no-explicit-any": "warn",
      "no-console": "off"
    }
  }
