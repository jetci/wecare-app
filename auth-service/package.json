{
  "name": "auth-service",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "lint": "npx eslint \"src/**/*.ts\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/server.ts",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^6.11.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "prisma": "^6.11.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/parser": "^8.35.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "globals": "^16.2.0",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.5.0",
